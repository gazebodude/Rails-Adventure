<!DOCTYPE html>
<html>
<head>
  <title>Adventure on Rails | <%= @title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= render 'layouts/navbar' %>
  <%= render 'layouts/header' %>

  <div class="main_view">
    <%= yield %>
  </div>

  <%= render 'layouts/footer' %>
  <%= debug(params) if Rails.env.development? %>
</body>
</html>
